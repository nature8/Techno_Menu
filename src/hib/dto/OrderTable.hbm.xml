<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="hibernate.beans">
    <class name="OrderTable" table="Ordertable">
        <id name="tableNo" column="TableNo"/>
        <property name="customerName" column="CustomerName"/>
        <property name="contactNumber" column="ContactNo"/>
        <property name="remark" column="review"/>
    </class>
</hibernate-mapping>


