<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="hibernate.beans">
    <class name="Bill" table="BillReview">
        <id name="custId" column="CustomerId"/>
        <property name="custName" column="CustomerName"/>
        <property name="contactNo" column="ContactNumber"/>
        <property name="custAddress" column="CustomerAddress"/>
        <property name="amount" column="amount"/>
        <property name="review" column="review"/>
    </class>
</hibernate-mapping>

